  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.43",
    "@types/react-redux": "^7.1.9",
    "codecov": "^3.7.2",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-redux": "^7.2.0",
