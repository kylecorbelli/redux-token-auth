{
  "name": "redux-token-auth",
  "version": "0.19.1",
  "description": "Redux actions and reducers to integrate with Devise Token Auth",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "node_modules/typescript/bin/tsc",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "{src,test}/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kylecorbelli/redux-token-auth.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "auth",
    "authentication",
    "token",
    "devise",
    "oauth"
  ],
  "author": "Kyle Corbelli",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "@types/react": "^16.7.9",
    "@types/react-native": "^0.57.15",
    "@types/react-redux": "^6.0.10",
    "axios": "^0.18.0",
    "react": "^16.6.3",
    "react-native": "^0.57.7",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1"
  }
}
